    "memory": "4",
    "disk": "20",
    "swap": "2",
    "url": "https://docs.graylog.org/en/3.3/pages/installation.html#system-requirements"
  },

  "user": {
    "username": "admin",
    "password": "admin"
  },

  "services":[
    {
      "name": "graylog",
      "port": "9001",
      "description_cn": "浏览器访问 Graylog",
      "description_en": "Browser access to Graylog Document by http",
      "required": "Y"
    }
  ]

}
