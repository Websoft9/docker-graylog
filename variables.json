{
  "name": "graylog",
  "trademark": "Graylog",
  "compose_file": "docker-compose.yml",
  "installpath": "/data/wwwroot/graylog",
  "env_file": ".env",
  "fork_url": "https://github.com/Graylog2/graylog-docker",
  "requirements":{
    "cpu_arch": "Linux x86-64, ARM 32/64, Windows x86-64, IBM POWER8, x86/i686",
